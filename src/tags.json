    "T0017": {
        "label": {
            "en": "intentional duplicate shot"
        },
        "helpMessage": {
            "en": "use this tag when the same original page has been digitized multiple times intentionally, for instance with various exposures to reveal details"
        },
        "ofField": "duplicate-of"
    },
    "T0018": {
        "label": {
            "en": "duplicate in artefact"
        },
        "helpMessage": {
            "en": "use this tag when the same page appears several times in the artefact (don't use it for different pages with the same pagination)"
        },
        "ofField": "duplicate-of"
    },
    "T0019": {
        "label": {
            "en": "missing from scans"
        },
        "helpMessage": {
            "en": "use this tag for pages that are in the artefact (in its present state of conservation) but which digitization has been overlooked"
        }
    },
    "T0020": {
        "label": {
            "en": "missing from artefact"
        },
        "helpMessage": {
            "en": "use this tag for pages that are missing in the artefact in its present state of conservation"
